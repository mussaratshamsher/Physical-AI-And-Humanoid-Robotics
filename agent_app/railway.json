{
  "name": "humanoid-robotics-agent",
  "services": [
    {
      "name": "agent-api",
      "type": "web",
      "runtime": "python",
      "env": {
        "PYTHON_VERSION": "3.11",
        "PORT": "8000"
      },
      "startCommand": "uvicorn app.main:app --host=0.0.0.0 --port=$PORT",
      "healthCheckPath": "/health"
    }
  ]
}